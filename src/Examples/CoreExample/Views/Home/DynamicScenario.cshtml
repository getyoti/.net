@{
    ViewData["Title"] = "DynamicScenario";
}
@model Yoti.Auth.ShareUrl.ShareUrlResult

<!DOCTYPE html>
<html>
<head>
    <title>Yoti Site</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.3/css/bootstrap.min.css" integrity="sha384-Zug+QiDoJOrZ5t4lssLdxGhVrurbmBWopoEl+M6BdEfwnCJZtKxi1KgxUyJq13dy" crossorigin="anonymous">
    <link rel="stylesheet" type="text/css" href="/static/style.css">

    <script src="https://sdk.yoti.com/clients/browser.2.2.0.js"></script>
</head>
<body>
    <div class="container">
        <h2>Dynamic Share Example</h2>
        <span data-yoti-type="inline"
              data-yoti-application-id="@ViewBag.YotiAppId"
              data-yoti-scenario-id="@ViewBag.ScenarioId"
              data-yoti-url="@Model.Url">
            Use Yoti
        </span>
    </div>
</body>
</html>